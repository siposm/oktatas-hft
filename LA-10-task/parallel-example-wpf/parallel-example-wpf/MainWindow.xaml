<Window x:Class="parallel_example_wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:parallel_example_wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Margin="40">
            <Button Click="Download" Content="DOWNLOAD" Margin="20" FontSize="40"></Button>
            <Label></Label>
            <Button Click="Cancel" Content="CANCEL" Margin="20" FontSize="40"></Button>
            <Label x:Name="result" Margin="20" FontSize="30" Height="60" Background="CadetBlue"></Label>
        </StackPanel>
    </Grid>
</Window>
